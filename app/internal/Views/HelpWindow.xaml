<Window x:Class="app.HelpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Help" Height="377" Width="394">
    <Grid>
        <TextBlock Text="Suppoted tokens"  FontSize="16"  FontWeight="Bold" HorizontalAlignment="Center" Margin="10" />
        <ListView ItemsSource="{Binding SupportedTokens}" Margin="10,40,10,10">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Token}" FontSize="16"/>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        
    </Grid>
</Window>
